<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.callforhelpdemu.Nav_UI.Home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.callforhelpdemu.Nav_UI.MyProfile.MyProfileFragment"
        android:label="ABOUT US"
        tools:layout="@layout/fragment_my_profile" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.callforhelpdemu.Nav_UI.AboutUs.AboutUsFragment"
        android:label="Sign out"
        tools:layout="@layout/fragment_about_us" />

    <fragment
        android:id="@+id/nav_tools"
        android:name="com.example.callforhelpdemu.Nav_UI.tools.ToolsFragment"
        android:label="Sign out"
        tools:layout="@layout/fragment_tools" />

    <fragment
        android:id="@+id/nav_share"
        android:name="com.example.callforhelpdemu.Nav_UI.Feedback.FeedbackFragment"
        android:label="@string/menu_share"
        tools:layout="@layout/fragment_feedback" />

    <fragment
        android:id="@+id/nav_send"
        android:name="com.example.callforhelpdemu.Nav_UI.Instruction.InstructionFragment"
        android:label="@string/menu_send"
        tools:layout="@layout/fragment_instruction" />
</navigation>